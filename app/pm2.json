{
  "apps": [
    {
      "name": "auto_deploy",
      "script": "app.js",
      "exec_mode": "cluster",
      "instances": 1,
      "watch": true,
      "ignore_watch": [
        "node_modules",
        "pid",
        "logs"
      ],
      "error_file": "./logs/err.log",
      "out_file": "./logs/out.log",
      "pid_file": "./pid/child.pid",
      "merge_logs": true
    }
  ]
}
